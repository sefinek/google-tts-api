    "google-apis",
    "google-cloud-platform",
    "google-cloud",
    "google-texttospeech",
    "google",
    "text-to-speech-api",
    "texttospeech"
  ],
  "homepage": "https://github.com/sefinek/google-tts-api#readme",
  "bugs": {
    "url": "https://github.com/sefinek/google-tts-api/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/sefinek/google-tts-api.git"
  },
  "license": "MIT",
  "author": "zlargon",
  "type": "commonjs",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "directories": {
    "example": "example",
    "test": "test"
  },
  "scripts": {
